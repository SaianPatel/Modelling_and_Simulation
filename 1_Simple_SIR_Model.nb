Cell[BoxData[
 RowBox[{
  RowBox[{"EulerForwardSolver", "[", 
   RowBox[{
   "initialState_", ",", " ", "A_", ",", " ", "B_", ",", " ", "stepSize_", 
    ",", " ", "steps_"}], "]"}], " ", ":=", " ", 
  RowBox[{"Module", "[", "\n", "    ", 
