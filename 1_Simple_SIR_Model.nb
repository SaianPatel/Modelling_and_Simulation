        RowBox[{"AppendTo", "[", 
         RowBox[{"trajectory", ",", " ", "state"}], "]"}], ";"}], "\n", 
       "        ", "\n", "        ", ",", " ", 
       RowBox[{"{", "steps", "}"}]}], "\n", "    ", "]"}], ";", "\n", "    ", 
     "\n", "    ", 
     RowBox[{"(*", " ", 
      RowBox[{
      "Return", " ", "the", " ", "trajectory", " ", "as", " ", "a", " ", 
       "list", " ", "of", " ", "states", " ", "over", " ", "time"}], " ", 
      "*)"}], "\n", "    ", "trajectory"}]}], "\n", "]"}]}]], "Code",
