     "}"}], ",", "\n", "    ", "\n", "    ", 
    RowBox[{"(*", " ", 
     RowBox[{
     "Initialize", " ", "the", " ", "state", " ", "and", " ", "trajectory"}], 
     " ", "*)"}], "\n", "    ", 
    RowBox[{
     RowBox[{"state", " ", "=", " ", "initialState"}], ";", "\n", "    ", 
     RowBox[{"trajectory", " ", "=", " ", 
      RowBox[{"{", "state", "}"}]}], ";", "\n", "    ", "\n", "    ", 
     RowBox[{"(*", " ", 
      RowBox[{
      "Iterate", " ", "for", " ", "the", " ", "specified", " ", "number", " ",
        "of", " ", "steps"}], " ", "*)"}], "\n", "    ", 
     RowBox[{"Do", "[", "\n", "        ", 
      RowBox[{"(*", " ", 
       RowBox[{
       "Define", " ", "the", " ", "nonlinear", " ", "term", " ", "for", " ", 
        "infection"}], " ", "*)"}], "\n", "        ", 
      RowBox[{
       RowBox[{
        RowBox[{"nonlinearTerm", " ", "=", " ", 
         RowBox[{
          RowBox[{"B", " ", ".", " ", "state"}], " ", "*", " ", 
          RowBox[{"state", "[", 
           RowBox[{"[", "2", "]"}], "]"}]}]}], ";", "  ", 
        RowBox[{"(*", " ", 
         RowBox[{
         "B", " ", "*", " ", "S", " ", "*", " ", "I", " ", "term", " ", "for",
           " ", "SIR", " ", "model"}], " ", "*)"}], "\n", "        ", "\n", 
        "        ", 
        RowBox[{"(*", " ", 
